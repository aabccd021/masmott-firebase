{
  "name": "masmott-firebase",
  "nazna": {
    "flake": false
  },
  "sideEffects": false,
  "scripts": {
    "build": "pnpm build:es6 && pnpm build:cjs && pnpm build:types && nazna build cli",
    "build:cjs": "swc src --out-dir dist/cjs --source-maps --config module.type=commonjs",
    "build:es6": "swc src --out-dir dist/es6 --source-maps",
    "build:types": "tsc --project tsconfig.dist.json",
    "fix": "eslint --max-warnings=0 --ext .ts . --fix",
    "lint": "eslint --max-warnings=0 --ext .ts .",
    "pre-push": "pnpm pre-push:dirty && pnpm publish --dry-run",
    "pre-push:dirty": "pnpm install && nazna fix && pnpm build && pnpm lint && pnpm test",
    "test": "firebase emulators:exec 'vitest --run --outputDiffLines 1000'"
  },
  "dependencies": {
    "@morphic-ts/batteries": "^3.0.0",
    "@morphic-ts/summoners": "^3.0.0",
    "firebase": "^9.14.0",
    "firebase-admin": "^11.3.0",
    "fp-ts": "^2.13.1",
    "fp-ts-std": "^0.15.1",
    "masmott": "^1.9.0",
    "ts-pattern": "^4.0.6"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.14",
    "eslint": "^8.27.0",
    "firebase-tools": "^11.16.1",
    "nazna": "^1.14.2",
    "node-fetch": "^3.3.0",
    "pnpm": "^7.14.2",
    "semantic-release": "^19.0.5",
    "serve": "^14.1.2",
    "typescript": "^4.8.4",
    "vitest": "^0.24.5"
  },
  "version": "0.0.0-semantic-release",
  "main": "./dist/cjs/index.js",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/es6/index.js"
  },
  "module": "./dist/es6/index.js",
  "files": [
    "dist"
  ],
  "types": "./dist/types/index.d.ts",
  "repository": "https://github.com/aabccd021/masmott-firebase.git",
  "license": "MIT",
  "bin": "./dist/cli.js"
}